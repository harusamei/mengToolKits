                            [ rdf:type owl:Class ;
                              owl:unionOf ( :Protestor
                                            :PublicProsecutor
                                          )
                            ] ;
            rdfs:comment "公诉人，检察官， 所有案件类型均可能出现，依法行使国家检察权的检察人员"@zh ;
            rdfs:label "检察官"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#ProtestingAuthority
:ProtestingAuthority rdf:type owl:Class ;
                     rdfs:subClassOf :JudicialAdministration ;
                     rdfs:comment "所有案件均可能出现，抗诉是指人民检察院对人民法院的审判实行法律监督的重要形式"@zh ;
                     rdfs:label "抗诉机关"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Protestor
:Protestor rdf:type owl:Class ;
           rdfs:subClassOf :JudicialStaff ;
