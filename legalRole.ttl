              rdfs:comment "比如律师执照的有效期，身份证的有效期"@zh ;
              rdfs:label "有效期"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#wChatID
:wChatID rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :chatID .


###  http://www.w3.org/2006/vcard/ns#countryName
vcard:countryName rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf foaf:name ;
                  rdfs:domain vcard:Location ;
                  rdfs:range xsd:string ;
                  rdfs:comment """无国籍人为“无”
取值可参考 iso-3166-1全球国家名称代码"""@zh ;
                  rdfs:label "国家名"@zh .


###  http://www.w3.org/2006/vcard/ns#email
vcard:email rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :contactInfo ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( foaf:Organization
                                        foaf:Person
                                      )
                        ] ;
            rdfs:range xsd:string ;
            rdfs:label "电子信件"@zh .


###  http://www.w3.org/2006/vcard/ns#formattedName
vcard:formattedName rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf foaf:name ;
                    rdfs:domain vcard:Location ,
                                foaf:Organization ;
                    rdfs:range xsd:string ;
                    rdfs:label "规范名称"@zh .


###  http://www.w3.org/2006/vcard/ns#mobileTel
vcard:mobileTel rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :contactInfo ;
                rdfs:range xsd:positiveInteger ;
                rdfs:label "移动电话"@zh .


###  http://www.w3.org/2006/vcard/ns#nickName
vcard:nickName rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf foaf:name ;
               rdfs:label "别名"@zh .


###  http://www.w3.org/2006/vcard/ns#photo
vcard:photo rdf:type owl:DatatypeProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( vcard:Location
                                        foaf:Organization
                                        foaf:Person
                                      )
                        ] .


###  http://www.w3.org/2006/vcard/ns#postalCode
vcard:postalCode rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :locationProperty ;
                 rdfs:domain vcard:Location ;
                 rdfs:range xsd:integer ;
                 rdfs:label "邮政编码"@zh .


###  http://www.w3.org/2006/vcard/ns#region
vcard:region rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :locationProperty ;
             rdfs:domain vcard:Location ;
             rdfs:range xsd:string ;
             rdfs:comment "取值 按行政区划"@zh ;
             rdfs:label "省市自治区"@zh .


###  http://www.w3.org/2006/vcard/ns#streetAddress
vcard:streetAddress rdf:type owl:DatatypeProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( vcard:Location
                                                foaf:Organization
                                              )
                                ] ;
                    rdfs:range xsd:string ;
                    rdfs:label "街区地址"@zh .


###  http://www.w3.org/2006/vcard/ns#tel
vcard:tel rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :contactInfo ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( foaf:Organization
                                      foaf:Person
                                    )
                      ] ;
          rdfs:label "固定电话"@zh .


###  http://xmlns.com/foaf/0.1/age
foaf:age rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :humanProperty ;
         rdfs:range xsd:integer ;
         rdfs:label "年龄"@zh .


###  http://xmlns.com/foaf/0.1/birthday
foaf:birthday rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :humanProperty ;
              rdfs:label "生日"@zh .


###  http://xmlns.com/foaf/0.1/gender
foaf:gender rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :humanProperty ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
                                     rdf:first "female" ;
                                     rdf:rest [ rdf:type rdf:List ;
                                                rdf:first "male" ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "unknown" ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                       ] ;
            rdfs:comment "枚举值：男，女，不详"@zh ;
            rdfs:label "性别"@zh .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          rdfs:comment """各种名称的父类
注： 语种，简称，全称 使用@
比如 @en @abbr @full ；分别表示 英文名， 简称， 全称"""@zh ;
          rdfs:label "名称"@zh .


###  http://xmlns.com/foaf/0.1/status
foaf:status rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://www.frdc.com/legalDocument/ontologies/legalRole#AccusedPerson
:AccusedPerson rdf:type owl:Class ;
               rdfs:subClassOf :Defendant ;
               rdfs:comment "被告，在刑事案件中称为被告人，即被公诉机关指控涉嫌犯罪的当事人"@zh ;
               rdfs:label "被告人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#ActingClerk
:ActingClerk rdf:type owl:Class ;
             rdfs:subClassOf :ParticipantInTrial ;
             rdfs:comment "临时代替书记员进行庭审记录工作的人员"@zh ;
             rdfs:label "代书记员"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#ActingJudge
:ActingJudge rdf:type owl:Class ;
             rdfs:subClassOf :Adjudicator ,
                             :MemberOfCollegialPanel ;
             rdfs:comment "人民法院内部临时代行审判员职务的助理审判员"@zh ;
             rdfs:label "代理审判员"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#ActingProcurator
:ActingProcurator rdf:type owl:Class ;
                  rdfs:subClassOf :Protestor ,
                                  :PublicProsecutor ;
                  rdfs:comment "所有案件类型均可能出现，代理检察官是因为工作需要，由上级检察院或者他同级检察院借调来的检察官"@zh ;
                  rdfs:label "代理检察员"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Adjudicator
:Adjudicator rdf:type owl:Class ;
             rdfs:subClassOf :JudicialStaff ;
             rdfs:comment "诉讼中，对案件进行审理和判决的机构和人"@zh ;
             rdfs:label "审判主体"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Agent
:Agent rdf:type owl:Class ;
       rdfs:comment """人或物在某种环境或情形下是什么， 附着的身份或地位
与foaf:Person，foaf:Organization相区别，是可变化的，侧重描述在上下文环境中人或物的地位和职责
相对地，foaf:Person，foaf:Organization指稳定、可见的实体"""@zh ;
       rdfs:label "角色身份, 行为主体，施事者，施动者"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Appellant
:Appellant rdf:type owl:Class ;
           rdfs:subClassOf :Litigant ;
           rdfs:comment "所有案件均可能出现，具有上诉权，并对第一审人民法院的裁判不服而提起上诉的主体"@zh ;
           rdfs:label "上诉人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Appellee
:Appellee rdf:type owl:Class ;
          rdfs:subClassOf :Litigant ;
          rdfs:comment "所有案件均可能出现，是指不服一审法院的裁判而提起上诉的人的对方当事人"@zh ;
          rdfs:label "被上诉人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Applicant
:Applicant rdf:type owl:Class ;
           rdfs:subClassOf :Litigant ;
           rdfs:comment """所有案件均可能出现
比如：民事特别程序申请人、撤销仲裁裁决申请人"""@zh ;
           rdfs:label "申请人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#ApplicantForRetrial
:ApplicantForRetrial rdf:type owl:Class ;
                     rdfs:subClassOf :Litigant ;
                     rdfs:comment "所有案件均可能出现，申请再审的当事人"@zh ;
                     rdfs:label "再审申请人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#ApplicationExecutor
:ApplicationExecutor rdf:type owl:Class ;
                     rdfs:subClassOf :Litigant ;
                     rdfs:comment "所有案件类型都可能出现，中向人民法院提出申请，要求根据生效法律文书，采取执行措施的当事人"@zh ;
                     rdfs:label "申请执行人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#AppointedAgent
:AppointedAgent rdf:type owl:Class ;
                rdfs:subClassOf :LegalAgent ;
                rdfs:comment """所有案件均可能出现，是指根据法院或指定单位的指定而产生代理权的代理。通常适用于两种情况：（1）在民事诉讼中，当事人系无民事行为能力或限制民事行为能力人，而又没有法定代理人的；（2）法定代理人之间互相推诿代理责任的，由法院指定其中一人为代理人
2017年《民法通则》中已经没有"""@zh ;
                rdfs:label "指定代理人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#AppraisalCenter
:AppraisalCenter rdf:type owl:Class ;
                 rdfs:subClassOf foaf:Organization ;
                 rdfs:label "鉴定中心"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Appraiser
:Appraiser rdf:type owl:Class ;
           rdfs:subClassOf :JudicalSupport ;
           rdfs:comment "所有案件类型都可能出现，在诉讼活动中鉴定人运用科学技术或者专门知识对诉讼涉及的专门性问题进行鉴别和判断并提供鉴定意见的活动"@zh ;
           rdfs:label "鉴定人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#ArbitrationCommission
:ArbitrationCommission rdf:type owl:Class ;
                       rdfs:subClassOf foaf:Organization ;
                       rdfs:comment "所有案件类型都可能出现，是独立、公正、高效地解决平等主体的公民、法人和其他组织之间发生的合同纠纷和其他财产权益纠纷的常设仲裁机构"@zh ;
                       rdfs:label "仲裁委员会"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#AssistantJudge
:AssistantJudge rdf:type owl:Class ;
                rdfs:subClassOf :Adjudicator ;
                rdfs:comment "所有案件都可能出现，是指专职审判辅助工作的司法人员"@zh ;
                rdfs:label "法官助理"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#AssistantProcurator
:AssistantProcurator rdf:type owl:Class ;
                     rdfs:subClassOf :Protestor ,
                                     :PublicProsecutor ;
                     rdfs:comment "所有案件类型均可能出现，虽然是检察官，自己可以独立办案，但是经验不足，资质还是尚浅，级别较低，主要还是协助主办检察官的工作"@zh ;
                     rdfs:label "助理检察员"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Bailiff
:Bailiff rdf:type owl:Class ;
         rdfs:subClassOf :JudicalSupport ;
         rdfs:comment "所有案件类型都可能出现，司法机关所设执行特定任务的警察"@zh ;
         rdfs:label "法警"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Certificate
:Certificate rdf:type owl:Class ;
             rdfs:subClassOf :Document ;
             rdfs:comment "比如 毕业证，合格证，文凭"@zh ;
             rdfs:label "证书"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#CertificateEvent
:CertificateEvent rdf:type owl:Class ;
                  rdfs:subClassOf :Event ;
                  rdfs:comment "可用于描述律师如何获得执业执照等各种有认证的证明文书"@zh ;
                  rdfs:label "获得证书事件"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Clerk
:Clerk rdf:type owl:Class ;
       rdfs:subClassOf :ParticipantInTrial ;
       rdfs:comment "所有案件都可能出现，是人民法院和人民检察院内担任办理案件的记录工作和有关事项的人员，并协助办理一系列司法辅助工作"@zh ;
       rdfs:label "书记员"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Company
:Company rdf:type owl:Class ;
         owl:equivalentClass :EnterpriseCorporation ;
         rdfs:subClassOf foaf:Organization ;
         rdfs:label "公司"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#CompensationClaimant
:CompensationClaimant rdf:type owl:Class ;
                      rdfs:subClassOf :Litigant ;
                      rdfs:comment "所有案件均可能出现，是指因行政机关及其工作人员违法执行职务而遭受损害，有权请求国家予以赔偿的人"@zh ;
                      rdfs:label "赔偿请求人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#CompensationCommission
:CompensationCommission rdf:type owl:Class ;
                        rdfs:subClassOf :ParticipantInTrial ;
                        rdfs:comment "是人民法院审理司法赔偿案件的审判组织"@zh ;
                        rdfs:label "赔偿委员会"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#CompensationOrgan
:CompensationOrgan rdf:type owl:Class ;
                   rdfs:subClassOf :JudicialAdministration ;
                   rdfs:comment "所有案件均可能出现，是指行使国家侦查、检察、审判、看守所及监狱管理职权的机关及其工作人员在行使职权时侵犯公民、法人和其他组织的合法权益造成损害的机关"@zh ;
                   rdfs:label "赔偿义务机关"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#CorporationForProfit
:CorporationForProfit rdf:type owl:Class ;
                      rdfs:subClassOf :LegalEntity ;
                      rdfs:label "盈利法人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Court
:Court rdf:type owl:Class ;
       rdfs:subClassOf :GovJudiciary ;
       rdfs:label "法院"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Criminal
:Criminal rdf:type owl:Class ;
          rdfs:subClassOf :Litigant ;
          rdfs:comment "刑事案件中出现，是指因犯罪而服刑役的人"@zh ;
          rdfs:label "罪犯"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Declarant
:Declarant rdf:type owl:Class ;
           rdfs:subClassOf :Litigant ;
           rdfs:comment "所有案件均可能出现，不服已经生效的裁决，向人民法院或者人民检察院提出的要求重新审理案件的主体"@zh ;
           rdfs:label "申诉人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Defendant
:Defendant rdf:type owl:Class ;
           rdfs:subClassOf :Litigant ;
           rdfs:comment """所有案件均可能涉及
在民事案件中，是指侵犯原告利益，需要追究民事责任，并经法院通知其应诉的人；
在行政诉讼中，公民、法人或其他组织认为某具体行政行为侵犯其合法权益而依法向人民法院提起的诉讼中，做出该具体行政行为的行政机关及人员。
在刑事案件中被称为被告人"""@zh ;
           rdfs:label "被告"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Defender
:Defender rdf:type owl:Class ;
          rdfs:subClassOf :EntrustedAgent ,
                          :LitigiousParticipant ;
          rdfs:comment "刑事案件中出现，指接受被追诉一方委托或者受人民法院指定，帮助犯罪嫌疑人、被告人行使辩护权以维护其合法权益的人"@zh ;
          rdfs:label "辩护人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#DefendingPartyToApplication
:DefendingPartyToApplication rdf:type owl:Class ;
                             rdfs:subClassOf :Litigant ;
                             rdfs:comment "所有案件均可能出现，与申请人相对应"@zh ;
                             rdfs:label "被申请人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Document
:Document rdf:type owl:Class ;
          rdfs:subClassOf :Object ;
          rdfs:label "文件"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#EducationEvent
:EducationEvent rdf:type owl:Class ;
                rdfs:subClassOf :Event ;
                rdfs:comment "用于描述某人受教育的过程，比如入学毕业地点等"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#EnterpriseCorporation
:EnterpriseCorporation rdf:type owl:Class ;
                       rdfs:subClassOf :CorporationForProfit ;
                       rdfs:label "企业法人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#EntrustedAgent
:EntrustedAgent rdf:type owl:Class ;
                rdfs:subClassOf :LegalAgent ;
                rdfs:comment """所有案件均可能出现，是指按照被代理人的委托行使代理权的人。可被委托的人包括：1、律师、法律服务工作者 2、当事人的近亲属或工作人员；3、当事人所在社区、单位以及有关社会团体推荐的公民
代理人的资格基于被代理人的单方授权"""@zh ;
                rdfs:label "委托代理人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Event
:Event rdf:type owl:Class ;
       rdfs:comment "事件的定义采用 cn.schema的定义， 详细见 https://schema.org.cn/Event"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#ExecutiveOrgan
:ExecutiveOrgan rdf:type owl:Class ;
                rdfs:subClassOf :JudicialAdministration ;
                rdfs:comment "所有案件类型都可能出现，负责执行的机关"@zh ;
                rdfs:label "执行机关"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#FarmHousehold
:FarmHousehold rdf:type owl:Class ;
               rdfs:subClassOf foaf:Group ;
               rdfs:label "农村承包经营户"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#GovJudiciary
:GovJudiciary rdf:type owl:Class ;
              rdfs:subClassOf :GovOrg ;
              rdfs:comment "包括：人民代表大会，国务院，中央军事委员会，地方各级人民代表大会和地方各级人民政府，民族自治地方的自治机关，人民法院，人民检察院"@zh ;
              rdfs:label "国家司法机关"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#GovOfficer
:GovOfficer rdf:type owl:Class ;
            rdfs:subClassOf :HumanRole ;
            rdfs:comment "Government officer, 指一切国家机关中从事公务的人员。比国家机关工作人员和国家公务员的范围宽"@zh ;
            rdfs:label "公务员"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#GovOrg
:GovOrg rdf:type owl:Class ;
        rdfs:subClassOf foaf:Organization ;
        rdfs:comment "government organization"@zh ;
        rdfs:label "国家机关"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#HumanRole
:HumanRole rdf:type owl:Class ;
           rdfs:subClassOf :Agent ;
           rdfs:comment "例： 职务是人大常委会副职领导，职级为 省部级的某人"@zh ;
           rdfs:label "人担任的角色"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#IdentifyingWitness
:IdentifyingWitness rdf:type owl:Class ;
                    rdfs:subClassOf :JudicalSupport ;
                    rdfs:comment "一般在刑事案件中出现，辨认重要当事人和物品文件的人"@zh ;
                    rdfs:label "辨认人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#IncidentalCivilActionDefendant
:IncidentalCivilActionDefendant rdf:type owl:Class ;
                                rdfs:subClassOf :Defendant ;
                                rdfs:comment "只在刑事案件中出现，是指附带民事诉讼中依法应当履行民事赔偿责任的人。通常就是刑事案件的被告人"@zh ;
                                rdfs:label "附带民事诉讼被告人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#IndictedPerson
:IndictedPerson rdf:type owl:Class ;
                rdfs:subClassOf :Litigant ;
                rdfs:comment "所有案件中均可能出现，被起诉人起诉的当事人"@zh ;
                rdfs:label "被起诉人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#IndividualBusiness
:IndividualBusiness rdf:type owl:Class ;
                    rdfs:subClassOf foaf:Group ;
                    rdfs:label "个体工商户"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Inspectors
:Inspectors rdf:type owl:Class ;
            rdfs:subClassOf :JudicalSupport ;
            rdfs:comment "所有案件类型都可能出现，主要是指对人、事、物等进行检查的人员"@zh ;
            rdfs:label "检验人员"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#Institution
:Institution rdf:type owl:Class ;
             rdfs:subClassOf foaf:Organization ;
             rdfs:label "事业单位"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#InstitutionCorporation
:InstitutionCorporation rdf:type owl:Class ;
                        rdfs:subClassOf :NonProfitCorporation ;
                        rdfs:label "事业单位法人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#InternClerk
:InternClerk rdf:type owl:Class ;
             rdfs:subClassOf :ParticipantInTrial ;
             rdfs:comment "一种处于实习状态的书记员"@zh ;
             rdfs:label "见习书记员"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#InvestigationAuthority
