@prefix : <http://www.frdc.com/legalDocument/ontologies/legalRole#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <https://www.w3.org/TR/owl-time/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@base <http://www.frdc.com/legalDocument/ontologies/legalRole> .

<http://www.frdc.com/legalDocument/ontologies/legalRole> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.frdc.com/legalDocument/ontologies/legalRole#actAs
:actAs rdf:type owl:ObjectProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( foaf:Organization
                                   foaf:Person
                                 )
                   ] ;
       rdfs:range :Agent ;
       rdfs:comment "比如某人作为被告"@zh ;
       rdfs:label "担任、作为"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#affiliates
:affiliates rdf:type owl:ObjectProperty ;
            rdfs:domain :HumanRole ;
            rdfs:range foaf:Organization ;
            rdfs:comment """任职于某组织
注：与memberOf区别，侧重在某环境下担当的工作"""@zh ;
            rdfs:label "任职于"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#disputes
:disputes rdf:type owl:ObjectProperty ;
          rdfs:domain :PrivateAgentInTrial ;
          rdfs:range :PrivateAgentInTrial ;
          rdfs:comment "有利益冲突的双方的关系，比如原先和被告"@zh ;
          rdfs:label "争执"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasAuthority
:hasAuthority rdf:type owl:ObjectProperty ;
              rdfs:domain foaf:Organization ;
              rdfs:range foaf:Organization ;
              rdfs:comment """主管机构指官方的权利机构，是用来管理该组织的机构，而不是该组织的总部
当局；官方；当权者the people or an organization who have the power to make decisions or who have a particular area of responsibility in a country or region"""@zh ;
              rdfs:label "有主管机构"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasBankAccount
:hasBankAccount rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( foaf:Organization
                                            foaf:Person
                                          )
                            ] ;
                rdfs:range :Company ;
                rdfs:label "有开户行"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasChild
:hasChild rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasKinship ;
          owl:inverseOf :hasParent ;
          rdf:type owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:label "有孩子"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasGroup
:hasGroup rdf:type owl:ObjectProperty ;
          rdfs:domain foaf:Organization ;
          rdfs:range foaf:Group ;
          rdfs:comment "比如公司有读书会，有党小组"@zh ;
          rdfs:label "有团体"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasHead
:hasHead rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://www.w3.org/ns/org#hasMember> ;
         rdfs:label "有领导或负责人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasKinship
:hasKinship rdf:type owl:ObjectProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain foaf:Person ;
            rdfs:range foaf:Person ;
            rdfs:comment """属性约束说明：不自反的
A不能是自己的亲属"""@zh ;
            rdfs:label "有亲属关系"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasLegalAgent
:hasLegalAgent rdf:type owl:ObjectProperty ;
               owl:inverseOf :isLegalAgent ;
               rdf:type owl:InverseFunctionalProperty ,
                        owl:IrreflexiveProperty ;
               rdfs:domain :PrivateAgentInTrial ;
               rdfs:range :LegalAgent ;
               rdfs:comment "比如原告有代理律师"@zh ;
               rdfs:label "有代理人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasLegalRepresentative
:hasLegalRepresentative rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://www.w3.org/ns/org#hasMember> ;
                        rdfs:label "有法人代表"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasLocality
:hasLocality rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Event
                                         foaf:Organization
                                       )
                         ] ;
             rdfs:range vcard:Location ;
             rdfs:comment "指有实际的地理位置"@zh ;
             rdfs:label "有地点"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasNativePlace
:hasNativePlace rdf:type owl:ObjectProperty ;
                rdfs:domain foaf:Person ;
                rdfs:range vcard:Location ;
                rdfs:comment "相比 nativePlaceName, 可以包含更详细的位置信息"@zh ;
                rdfs:label "籍贯是"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasOffice
:hasOffice rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasLocality ;
           rdfs:domain foaf:Organization ;
           rdfs:range vcard:Location ;
           rdfs:label "有办公室"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasParent
:hasParent rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasKinship ;
           rdf:type owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:range foaf:Person ;
           rdfs:label "有父母"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#hasRegisteredSite
:hasRegisteredSite rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasLocality ;
                   rdfs:domain foaf:Organization ;
                   rdfs:range vcard:Location ;
                   rdfs:comment "公司的注册地址"@zh ;
                   rdfs:label "注册地点"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#holdsConfirmation
:holdsConfirmation rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :HumanRole
                                               foaf:Person
                                             )
                               ] ;
                   rdfs:range :Object ;
                   rdfs:comment "比如护照，户口，身份证等"@zh ;
                   rdfs:label "持有的（身份）确认证书"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#isAttendee
:isAttendee rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Agent
                                        foaf:Person
                                      )
                        ] ;
            rdfs:range :Event ;
            rdfs:comment "比如观看演出的观众"@zh ;
            rdfs:label "是参与者"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#isLegalAgent
:isLegalAgent rdf:type owl:ObjectProperty ;
              rdfs:domain :LegalAgent ;
              rdfs:label "是某人的代理人"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#isMarital
:isMarital rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasKinship ;
           rdf:type owl:SymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:range foaf:Person ;
           rdfs:label "是夫妻关系的"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#isPerformer
:isPerformer rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Agent
                                         foaf:Person
                                       )
                         ] ;
             rdfs:range :Event ;
             rdfs:comment "事件的主体，如获得执业资格的律师，在获得执照事件中"@zh ;
             rdfs:label "事件的主要执行者"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#occuresAt
:occuresAt rdf:type owl:ObjectProperty ;
           rdfs:domain :Event ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( vcard:Location
                                      foaf:Organization
                                    )
                      ] ;
           rdfs:comment "比如在大学读书事件中的大学"@zh ;
           rdfs:label "事件发生的场所，可以是地点或某机构"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#resides
:resides rdf:type owl:ObjectProperty ;
         rdfs:domain foaf:Person ;
         rdfs:range vcard:Location ;
         rdfs:label "居住于"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#serveAs
:serveAs rdf:type owl:ObjectProperty ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( foaf:Group
                                     foaf:Organization
                                   )
                     ] ;
         rdfs:range :Agent ;
         rdfs:comment "比如某公司作为原告"@zh ;
         rdfs:label "充当、作为、行使"@zh .


###  http://www.w3.org/ns/org#hasMember
<http://www.w3.org/ns/org#hasMember> rdf:type owl:ObjectProperty ;
                                     owl:inverseOf <http://www.w3.org/ns/org#memberOf> ;
                                     rdfs:domain [ rdf:type owl:Class ;
                                                   owl:unionOf ( foaf:Group
                                                                 foaf:Organization
                                                               )
                                                 ] ;
                                     rdfs:range foaf:Person ;
                                     rdfs:label "有成员"@zh .


###  http://www.w3.org/ns/org#hasSubOrg
<http://www.w3.org/ns/org#hasSubOrg> rdf:type owl:ObjectProperty ;
                                     owl:inverseOf <http://www.w3.org/ns/org#hasSupOrg> ;
                                     rdfs:domain foaf:Organization ;
                                     rdfs:range foaf:Organization ;
                                     rdfs:label "有下级机构"@zh .


###  http://www.w3.org/ns/org#hasSupOrg
<http://www.w3.org/ns/org#hasSupOrg> rdf:type owl:ObjectProperty ;
                                     rdfs:label "有上级机构"@zh .


###  http://www.w3.org/ns/org#headOf
<http://www.w3.org/ns/org#headOf> rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf <http://www.w3.org/ns/org#memberOf> ;
                                  rdfs:range [ rdf:type owl:Class ;
                                               owl:unionOf ( foaf:Group
                                                             foaf:Organization
                                                           )
                                             ] ;
                                  rdfs:label "是某组织的负责人"@zh .


###  http://www.w3.org/ns/org#memberOf
<http://www.w3.org/ns/org#memberOf> rdf:type owl:ObjectProperty ;
                                    rdfs:range [ rdf:type owl:Class ;
                                                 owl:unionOf ( foaf:Group
                                                               foaf:Organization
                                                             )
                                               ] ;
                                    rdfs:comment "是某机构成员"@zh ;
                                    rdfs:label "就职于，加入"@zh .


###  http://xmlns.com/foaf/0.1/knows
foaf:knows rdf:type owl:ObjectProperty ,
                    owl:SymmetricProperty ;
           rdfs:domain foaf:Person ;
           rdfs:range foaf:Person ;
           rdfs:label "相互认识"@zh .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/identifier
dct:identifier rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:range xsd:token ;
               rdfs:label "ID号"@zh .


###  http://purl.org/dc/terms/type
dct:type rdf:type owl:DatatypeProperty .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#HPUrl
:HPUrl rdf:type owl:DatatypeProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( foaf:Organization
                                   foaf:Person
                                 )
                   ] ;
       rdfs:range xsd:anyURI ;
       rdfs:label "官网地址"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#aboutOrg
:aboutOrg rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :orgProperty ;
          rdfs:domain foaf:Organization ;
          rdfs:range xsd:string ;
          rdfs:comment "组织介绍"@zh ;
          rdfs:label "简介"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#approvalNum
:approvalNum rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :eventProperty ;
             rdfs:comment """比如律所的执业许可证的批准文号
沪司审(04-1)字(2013)1547号"""@zh ;
             rdfs:label "批准文号"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#assessment
:assessment rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :eventProperty ;
            rdfs:domain :PracticeEvent ;
            rdfs:comment """例如，上级对律师执业情况的评估，考核， 可能是年审
2016年基本称职
2017年暂缓考核"""@zh ;
            rdfs:label "评估"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#award
:award rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :humanProperty ;
       rdfs:comment "比如获得国家五一奖章，奥斯卡等"@zh ;
       rdfs:label "获奖或荣誉"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#behavioralAbility
:behavioralAbility rdf:type owl:DatatypeProperty ;
                   rdfs:domain :PrivateAgentInTrial ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:oneOf [ rdf:type rdf:List ;
                                            rdf:first "完全行为能力" ;
                                            rdf:rest [ rdf:type rdf:List ;
                                                       rdf:first "无行为能力" ;
                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "限制行为能力" ;
                                                                  rdf:rest rdf:nil
                                                                ]
                                                     ]
                                          ]
                              ] ;
                   rdfs:label "行为能力"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#businessHour
:businessHour rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :orgProperty ;
              rdfs:domain foaf:Organization ;
              rdfs:comment """基本格式
open:tt,close:tt"""@zh ;
              rdfs:label "营业时间"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#businessLicenseNum
:businessLicenseNum rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf dct:identifier ;
                    rdfs:label "营业执照"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#businessScope
:businessScope rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :orgProperty ;
               rdfs:domain foaf:Organization ;
               rdfs:label "营业范围"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#businessSpeciality
:businessSpeciality rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :orgProperty ;
                    rdfs:domain foaf:Organization ;
                    rdfs:label "业务特长"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#certificateName
:certificateName rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :eventProperty ,
                                    foaf:name ;
                 rdfs:domain :CertificateEvent ;
                 rdfs:label "证书名称"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#certificateStatus
:certificateStatus rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :eventProperty ;
                   rdfs:comment "比如，正常，失效等"@zh ;
                   rdfs:label "证书状态"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#certificateType
:certificateType rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf dct:type ,
                                    :eventProperty ;
                 rdfs:domain :CertificateEvent ;
                 rdfs:label "证书类型"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#chatID
:chatID rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf :contactInfo ;
        rdfs:label "聊天帐号"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#commend
:commend rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :eventProperty ;
         rdfs:domain :PracticeEvent ;
         rdfs:label "表彰"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#contactInfo
:contactInfo rdf:type owl:DatatypeProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( foaf:Organization
                                         foaf:Person
                                       )
                         ] ;
             rdfs:label "联系信息"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#countOfLawyer
:countOfLawyer rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :lawFirmProperty ;
               rdfs:domain :LawFirm ;
               rdfs:range xsd:integer .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#countOfMember
:countOfMember rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :orgProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( foaf:Group
                                           foaf:Organization
                                         )
                           ] ;
               rdfs:range xsd:integer ;
               rdfs:comment "机构人数"@zh ;
               rdfs:label "人数"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#criminalResponsibility
:criminalResponsibility rdf:type owl:DatatypeProperty ;
                        rdfs:domain :PrivateAgentInTrial ;
                        rdfs:label "刑事责任能力"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#dataSecurity
:dataSecurity rdf:type owl:DatatypeProperty ;
              rdfs:label "信息安全要求的属性"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#dateOfComplaint
:dateOfComplaint rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :eventProperty ;
                 rdfs:domain :PracticeEvent ;
                 rdfs:label "被投诉的时间"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#dateOfPrevious
:dateOfPrevious rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :eventProperty ;
                rdfs:label "上一个证书获得的时间"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#description
:description rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :eventProperty ;
             rdfs:comment "比如 trigger words 等"@zh ;
             rdfs:label "事件的简单描述"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#domicilePlaceName
:domicilePlaceName rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :humanProperty ;
                   rdfs:range xsd:string ;
                   rdfs:label "户籍地名"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#domicileRegisterNum
:domicileRegisterNum rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :humanProperty ;
                     rdfs:label "户口簿编号"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#education
:education rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :humanProperty ;
           rdfs:comment """参照国家标准《GB/T 4658-2006 学历代码》
枚举值：10:研究生教育，11:博士研究生毕业，12:博士研究生结业，13:博士研究生肄业，14:硕士研究生毕业，15:硕士研究生结业，16:硕士研究生肄业，17:研究生班毕业，18:研究生班结业，19:研究生班肄业，20:大学本科，30:专科教育，21:大学本科毕业，22:大学本科结业，23:大学本科肄业，28:大学普通班毕业，31:大学专科毕业，32:大学专科结业，33:大学专科肄业，40:中等职业教育，41:中等专科毕业，42:中等专科结业，43:中等专科肄业，44:职业高中毕业，45:职业高中结业，46:职业高中肆业，47:技工学校毕业，48:技工学校结业，49:技工学校肄业，60:普通高级中学教育，61:普通高中毕业，62:普通高中结业，63:普通高中肄业，70:初级中学教育，71:初中毕业，73:初中肄业，80:小学教育，81:小学毕业，83:小学肆业，90:其他"""@zh ;
           rdfs:label "文化程度"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#endDate
:endDate rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :eventProperty ;
         rdfs:label "结束日期"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#enterpriseCode
:enterpriseCode rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf dct:identifier ;
                rdfs:comment "事业法人登记证书的编号"@zh ;
                rdfs:label "事业法人登记码"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#ethnicGroup
:ethnicGroup rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :humanProperty ;
             rdfs:label "民族"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#eventName
:eventName rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :eventProperty ,
                              foaf:name ;
           rdfs:comment "比如， 被投诉， 成为合伙人等"@zh ;
           rdfs:label "事件名"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#eventProperty
:eventProperty rdf:type owl:DatatypeProperty ;
               rdfs:domain :Event ;
               rdfs:label "事件相关属性"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#eventType
:eventType rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf dct:type ,
                              :eventProperty ;
           rdfs:comment "暂时包含： 教育， 工作， 获得资格， 商务， 职位变动"@zh ;
           rdfs:label "事件类型"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#fax
:fax rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf :contactInfo ;
     rdfs:label "传真"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#firstOccuredPlaceName
:firstOccuredPlaceName rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :eventProperty ;
                       rdfs:label "首次发生的地名"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#foundedDate
:foundedDate rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :orgProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( foaf:Group
                                         foaf:Organization
                                       )
                         ] ;
             rdfs:label "成立日期"@zh .


###  http://www.frdc.com/legalDocument/ontologies/legalRole#geo
